var ketsuConsoleLogs = [];
console.defaultLog = console.log.bind(console);
console.log = function()
{
let data = JSON.stringify(arguments["0"],null,2)
    console.everything.push(
    {
        'type': 'log',
        'value': data
    });
    ketsuConsoleLogs.push(
    {
        'type': 'log',
        'value': data
    });
    console.defaultLog.apply(console, arguments)
};

console.error = function()
{
let data = JSON.stringify(arguments["0"],null,2)
    console.everything.push(
    {
        'type': 'error',
        'value': data
    });
    ketsuConsoleLogs.push(
    {
        'type': 'error',
        'value': data
    });
    console.defaultLog.apply(console, arguments)
};
